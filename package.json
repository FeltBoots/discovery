{
    "name": "@discoveryjs/discovery",
    "version": "1.0.0-beta.46",
    "description": "Hackable JSON discovery tool",
    "author": "Roman Dvornov <rdvornov@gmail.com> (https://github.com/lahmatiy)",
    "license": "MIT",
    "repository": "discoveryjs/discovery",
    "keywords": [],
    "main": "dist/discovery.umd.js",
    "unpkg": "dist/discovery.umd.js",
    "jsdelivr": "dist/discovery.umd.js",
    "browser": {
        "/gen/jora.js": "./dist/gen/jora.js",
        "/gen/@discoveryjs/json-ext.js": "./dist/gen/@discoveryjs/json-ext.js",
        "/gen/hitext": "./dist/gen/hitext.js",
        "/gen/hitext-prismjs": "./dist/gen/hitext-prismjs.js",
        "/gen/prismjs": "./dist/gen/prismjs.js",
        "/gen/codemirror": "./dist/gen/codemirror.js"
    },
    "scripts": {
        "lint": "eslint scripts src",
        "build:js": "esbuild src/lib.js --outfile=dist/lib.js --bundle --define:global=window --format=esm --minify --sourcemap",
        "build": "npm run build:js && npm run build:css",
        "build-docs": "npm run build && discovery-build -o docs --isolate-styles",
        "build:css": "node scripts/build-css",
        "prepublishOnly": "npm run build",
        "cypress": "npx cypress open",
        "start:server": "npx discovery -c ./cypress/fixtures/single-model/.discoveryrc.js",
        "test": "./cypress/test.sh"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "dependencies": {
        "@discoveryjs/json-ext": "^0.3.2",
        "codemirror": "^5.52.2",
        "hitext": "^1.0.0-beta.1",
        "hitext-prismjs": "^1.1.0",
        "jora": "1.0.0-beta.3",
        "prismjs": "^1.21.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@discoveryjs/cli": "^1.13.0",
        "babelify": "^10.0.0",
        "browserify": "^16.5.0",
        "cypress": "^5.3.0",
        "esbuild": "^0.7.21",
        "eslint": "^6.8.0",
        "eslint-plugin-cypress": "^2.11.2",
        "eswatch": "^0.6.0",
        "terser": "^5.3.3"
    },
    "files": [
        "dist",
        "libs",
        "src"
    ]
}
